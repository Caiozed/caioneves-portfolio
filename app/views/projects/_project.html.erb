<h2 class="header-text-color content-title">
    <%= project.name %><%= link_to "Edit", edit_project_path(project), class:"btn btn-info"%>
    <%= link_to "Delete", project_path(project), class:"btn btn-info", method: :delete, data: {confirm: "Are you sure?"}%>
</h2>
<%= image_tag project.image.url %>
<p><%= project.description %></p>
<%= link_to "Git", project.git_url, class: "btn btn-info", target: "_blank" %>
<%= link_to "Live Demo", project.demo_url, class: "btn btn-info", target: "_blank" %>

