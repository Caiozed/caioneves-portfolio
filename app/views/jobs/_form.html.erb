<%= form_for @job do |f|%>
    <%= f.label :job_name%>
    <%= f.text_field :name %>
    
    <%= f.label :start_date %>
    <%= f.date_field :start_date %>
    
    <%= f.label :end_date %>
    <%= f.date_field :end_date %>
    
    <%= f.label :description %>
    <%= f.text_area :description %>
    
    <%= f.submit class:"btn btn-primary" %>
<% end %>